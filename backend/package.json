{
  "name": "lol-pick-ban-ui",
  "version": "0.0.0",
  "private": true,
  "main": "electron-main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node backend-build/app.js",
    "lint": "eslint backend/**",
    "electron-dev": "npm run build && electron .",
    "ncc": "ncc build app.ts -o backend-build --source-map",
    "package": "npm run ncc && pkg backend-build/index.js --targets node22-win-x64 --public --output PickBanUIBackend",
    "release": "npm run package && release.bat"
  },
  "pkg": {
    "scripts": "dist/*.js",
    "assets": [
      "config.json"
    ]
  },
  "dependencies": {
    "@types/fs-extra": "11.0.4",
    "axios": "^1.11.0",
    "cli-progress": "^3.12.0",
    "cookie-parser": "~1.4.7",
    "debug": "~4.4.1",
    "express": "~5.1.0",
    "fs-extra": "^11.3.0",
    "lcu-connector": "^2.1.4",
    "minimist": "^1.2.8",
    "morgan": "~1.10.1",
    "needle": "^3.3.1",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.3",
    "@types/minimist": "^1.2.5",
    "@types/needle": "^3.3.0",
    "@types/node": "24.3.0",
    "@types/ws": "^8.18.1",
    "@yao-pkg/pkg": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0"
  }
}
